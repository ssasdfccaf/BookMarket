package ch04.com.dao;

public class Person {
	private ind id = 20181004;
	private String name = "홍길순";
	
	public Person() {
		
	}
	
	public int getId() {
		return id;
	}
	
	public void setId(int id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
